{
    "name": "archytech/laramix",
    "description": "Informix Driver for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ifx",
        "informix",
        "laravel",
        "laravel 6",
        "laravel 7",
        "laravel-framework"
    ],
    "authors": [
        {
            "name": "Arief BP",
            "email": "ariefbp27@gmail.com",
            "homepage": "https://github.com/archytech99/laramix"
        }
    ],
    "require": {
        "php": "^6.0|^7.0",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/pagination": "^6.0|^7.0",
        "illuminate/encryption": "^6.0|^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.10",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Archytech\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Archytech\\Laravel\\Ifx\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Archytech\\Laravel\\Ifx\\InformixServiceProvider"
            ]
        }
    }
}
